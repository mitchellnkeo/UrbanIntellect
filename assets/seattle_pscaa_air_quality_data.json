  "usage_examples": {
    "javascript": {
      "load_data": "const airQualityData = await fetch('./seattle_pscaa_air_quality_traffic_data.json').then(r => r.json());",
      "get_summary": "const summary = airQualityData.air_quality_summary;",
      "get_sensors": "const sensors = airQualityData.sensors;",
      "filter_by_traffic": "const highTraffic = sensors.filter(s => s.traffic_impact.includes('High'));",
      "filter_by_category": "const moderate = sensors.filter(s => s.air_quality.category === 'Moderate');",
      "get_traffic_analysis": "const trafficAnalysis = airQualityData.traffic_impact_analysis;"
    },
    "react": {
      "useState": "const [airQualityData, setAirQualityData] = useState(null);",
      "useEffect": "useEffect(() => { fetch('./seattle_pscaa_air_quality_traffic_data.json').then(r => r.json()).then(setAirQualityData); }, []);",
      "map_sensors": "sensors.map(sensor => <div key={sensor.station_id}>{sensor.station_name}: {sensor.air_quality.overall_aqi} AQI ({sensor.traffic_impact})</div>);"
    }
  }
