  "usage_examples": {
    "javascript": {
      "load_data": "const airQualityData = await fetch('./seattle_pscaa_air_quality_data.json').then(r => r.json());",
      "get_summary": "const summary = airQualityData.air_quality_summary;",
      "get_sensors": "const sensors = airQualityData.sensors;",
      "filter_by_category": "const goodSensors = sensors.filter(s => s.air_quality.category === 'Good');",
      "get_best_air_quality": "const best = sensors.sort((a,b) => a.air_quality.overall_aqi - b.air_quality.overall_aqi);"
    },
    "react": {
      "useState": "const [airQualityData, setAirQualityData] = useState(null);",
      "useEffect": "useEffect(() => { fetch('./seattle_pscaa_air_quality_data.json').then(r => r.json()).then(setAirQualityData); }, []);",
      "map_sensors": "sensors.map(sensor => <div key={sensor.station_id}>{sensor.station_name}: {sensor.air_quality.overall_aqi} AQI</div>);"
    }
  }
